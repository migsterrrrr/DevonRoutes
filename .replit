modules = ["python-3.11", "postgresql-16"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["osm2pgsql"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Import OSM Data"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Setup Routing"

[[workflows.workflow]]
name = "Import OSM Data"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python import_osm.py"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Setup Routing"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python setup_routing.py"

[workflows.workflow.metadata]
outputType = "console"
